{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "react-dev-2.0",
  "author": "GovTech",
  "module": "dist/react-dev-2.0.esm.js",
  "size-limit": [
    {
      "path": "dist/react-dev-2.0.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-dev-2.0.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@size-limit/preset-small-lib": "^7.0.4",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addons": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@types/enzyme": "^3.10.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-transition-group": "^4.4.4",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "babel-loader": "^8.2.3",
    "bootstrap-icons": "^1.7.2",
    "enzyme": "^3.11.0",
    "husky": "^7.0.4",
    "prop-types-extra": "^1.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "sinon": "^12.0.1",
    "size-limit": "^7.0.4",
    "storybook-dark-mode": "^1.0.8",
    "storybook-readme": "^5.0.9",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@restart/ui": "^0.2.5",
    "@storybook/addon-a11y": "^6.4.9",
    "@storybook/addon-storysource": "^6.4.9",
    "@types/invariant": "^2.2.35",
    "@types/react-transition-group": "^4.4.4",
    "classnames": "^2.3.1",
    "formik": "^2.2.9",
    "invariant": "^2.2.4",
    "react-transition-group": "^4.4.2",
    "yup": "^0.32.11"
  },
  "jest": {
    "globals": {
      "__DEV__": true
    },
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.ts"
    ],
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "./coverage/"
  }
}

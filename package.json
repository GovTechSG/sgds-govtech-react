    "react-transition-group": "^4.4.2",
    "warning": "^4.0.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "overrides": {
    "nwsapi": "2.2.2"
  }
}
